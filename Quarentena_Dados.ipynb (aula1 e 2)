        "def plot_filme(n):\n",
        "   notas_do_filme = avaliacoes.query(f\"filmeId=={n}\")[\"nota\"]\n",
        "   notas_do_filme.plot(kind='hist')\n",
        "   plt.show()\n",
        "   notas_do_filme.plot.box()\n",
        "   plt.show()\n",
        "   return notas_do_filme.describe()\n",
        "\n",
